# GPU Server Environment Configuration
# Copy this file to .env and configure for your deployment

# Server Configuration
GPU_SERVER_PORT=3000
LOG_LEVEL=INFO

# Authentication
GPU_SERVER_API_KEY=your_secure_api_key_here

# Storage Paths
MODEL_CACHE_DIR=/workspace/models
RESULT_STORAGE_DIR=/tmp/gpu_results

# Worker Configuration
WORKER_TIMEOUT=300

# Optional: CUDA Configuration
# CUDA_VISIBLE_DEVICES=0
